@{
    Layout = null;
}
@{
    String userType = ViewBag.UserTypeName;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Dashboard</title>
    <link rel="shortcut icon" href="~/Assets/images/favicon.ico">

    <!-- Bootstrap -->
    <link href="~/Assets/bootstrap-3.1.0/css/bootstrap.min.css" rel="stylesheet" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
     <script src="~/Assets/bootstrap-3.1.0/html5shiv.js"></script>
     <script src="~/Assets/bootstrap-3.1.0/respond.min.js"></script>
    <![endif]-->

    <link href="~/Assets/plugins/forms/validate/validate.css" rel="stylesheet" />
    <link href="~/Assets/css/color.css" rel="stylesheet" />
    <link href="~/Assets/plugins/forms/select/select2.css" rel="stylesheet" />
    <link href="~/assets/css/supr-theme/jquery.ui.supr.css" rel="stylesheet" />
    <link href="~/assets/css/my-css.css" rel="stylesheet" />
    <link href="~/Assets/css/font-awesome-4.0.3/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Assets/plugins/misc/pnotify/jquery.pnotify.default.css" rel="stylesheet" />
    <style>
        .hide-this {
            display: none;
        }
    </style>


</head>

<body>

    <!-- navigation bar--->
    <div class="navbar navbar-inverse navbar-static-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                 <a class="navbar-brand" href="#">@ViewBag.DashboardTitle</a>

            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="dropdown active">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" title="Menu"><span class="glyphicon glyphicon-th"></span>&nbsp;Menu<b class="caret"></b></a>
                        <ul class="dropdown-menu">

                            @{
                                switch(userType)
                                {
                                    case "sr":
                                        <text>
                                            <li><a class="handle-link" href="~/SR/SRMemoEntry"><span class="glyphicon glyphicon-file"></span>&nbsp;&nbsp;New Cash Memo</a></li>
                                            <li><a class="handle-link" href="~/SR/SearchMemoHistories"><span class="glyphicon glyphicon-list-alt"></span>&nbsp;&nbsp;Cash Memo History</a></li>
                            
                                        </text>
                                        break;

                                    case "cms":
                                        <text>
                                            <li><a id="cmsDefaultPage" href="~/CMSupervisor/SupervisorLandingPage" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>CM Schedule</a></li>  
                                            <li class="divider"></li>
                                            <li class="dropdown-header">Other Link</li>
                                            <li><a href="~/Dashboard?m=n&switch=y&user=@ViewBag.LoginName"><span class="glyphicon glyphicon-road"></span>&nbsp;&nbsp;Admin Panel</a></li>
                                        </text>
                                        break;
       
                                        
                                     case "tsa":
                  <text>
                  <li><a id="tsaDefaultPage" href="~/TSAActivity/TSALandingPage" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>My Schedule</a></li>  
                  <li><a  href="~/TSAActivityWOSR/SearchTargetOutletPage" class="handle-link" ><span class="icon16 icomoon-icon-arrow-right-3"></span>Search Target Outlet</a></li>
                  <li><a href="~/TSARoutePlan/MyRoutePlan" class="handle-link"><span class="icon16 icomoon-icon-arrow-right-3"></span>Create Route Plan</a></li>
                  <li><a href="~/TSAActivityWOSR/SyncSRData" class="handle-link"><span class="icon16 icomoon-icon-arrow-right-3"></span>Sync SR Data</a></li>
                  <li class="divider"></li>
                  <li class="dropdown-header">Memo</li>                      
                  <li><a class="handle-link" href="~/SR/SRMemoEntry"><span class="glyphicon glyphicon-file"></span>&nbsp;&nbsp;New Cash Memo</a></li>
                  <li><a class="handle-link" href="~/SR/SearchMemoHistories"><span class="glyphicon glyphicon-list-alt"></span>&nbsp;&nbsp;Cash Memo History</a></li>
                  <li class="divider"></li>
                  <li class="dropdown-header">Other Link</li>
                  <li><a href="~/Dashboard?m=n&switch=y&user=@ViewBag.LoginName"><span class="glyphicon glyphicon-road"></span>&nbsp;&nbsp;Admin Panel</a></li>
                  </text>
                   break;

                 case "brs":
                 <text>
                 <li><a href="~/BRSupervisor/Index" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>My Route Plan</a></li>
                 <li><a href="~/BRSupervisor/BRSupervisorFieldVisits" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>Field Visit</a></li>
                 <li><a href="~/BRSupervisor/Index" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>BR List</a></li>
                 <li><a href="~/BRSupervisor/BRRoutePlan" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>BR Route Plan</a></li>
                 <li><a href="~/BRSupervisor/ViewRoutePlans" class="handle-link"><span class="icon16 icomoon-icon-table-2"></span>View Route Plans</a></li>                                          
                 <li class="divider"></li>
                 <li class="dropdown-header">Other Link</li>
                 <li><a href="~/Dashboard?m=n&switch=y&user=@ViewBag.LoginName"><span class="glyphicon glyphicon-road"></span> Admin panel</a></li>
                 </text>
                   break;
                 default:
                    break;
                          }
                            }


                           




                        </ul>
                    </li>

                    <li><a class="handle-link" href="~/Common/ReportPosition?user=@ViewBag.LoginName"><span class="glyphicon glyphicon-map-marker"></span>&nbsp;Report Position</a></li>
                </ul>

                
                <!--Right hand menu starts-->
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown"><span class="glyphicon glyphicon-user"></span>&nbsp;@ViewBag.FullName<b class="caret"></b></a>
                        <ul class="dropdown-menu">
         @*                   <li><a href="#"><span class="glyphicon glyphicon-list-alt"></span>&nbsp;&nbsp;Profile</a></li>
                            <li class="divider"></li>*@
                            <li><a href="~/Common/ChangePassword?userid=@ViewBag.UserId" class="handle-link"><span class="glyphicon glyphicon-floppy-open"></span>&nbsp;&nbsp;Change Password</a></li>
                            <li class="divider"></li>

                            <li><a href="/Login?mode=logout&m=y"><span class="glyphicon glyphicon-log-out"></span>&nbsp;&nbsp;Logout</a></li>
                        </ul>
                    </li>
                </ul>
                <!--Right hand menu ends-->


            </div>
            <!--/.navbar-collapse -->
        </div>
    </div>
    <!-- navigation bar--->


    <div class="container">

        <div id="PartialContent">

       </div>
    </div>
    <!-- /container -->

   

   <script src="~/Assets/jquery-2.0.3/jquery-2.0.3.min.js"></script>
    <script src="~/Assets/bootstrap-3.1.0/js/bootstrap.min.js"></script>

   
    <script src="https://maps.googleapis.com/maps/api/js?sensor=false"></script>

   <script src="~/Assets/plugins/forms/validate/jquery.validate.min.js"></script>
  
    <script src="~/assets/ajax/libs/jqueryui/1.10.2/jquery-ui.min.js" type="text/javascript"></script>
    <script src="~/Assets/plugins/forms/select/select2.min.js"></script>
    <script src="~/assets/plugins/tables/dataTables/jquery.dataTables.min.js"></script>
    <script src="~/Assets/js/notifications.js"></script>
    <script src="~/Assets/plugins/misc/pnotify/jquery.pnotify.min.js"></script>
    <script src="~/Assets/js/handle-link-mobile.js"></script>
    <script type="text/javascript" src="../assets/plugins/forms/select/select2.min.js"></script>

    

    <!-- load default page for current user-->
        <script>
            $(function () {

                var userTypeName = '@ViewBag.UserTypeName';
                var DefaultLinkIdentifier = $('#' + userTypeName + 'DefaultPage');

                //if this identifier exists
                if (DefaultLinkIdentifier.length) {
                    var aTag = DefaultLinkIdentifier.closest('a');
                    var defaultLink = aTag.attr('href');
                    $(aTag).addClass('current');

                    $.ajax({
                        url: defaultLink,
                        async: true,
                        beforeSend: function () {
                            $("#PartialContent").fadeOut("slow").empty();
                        },
                        success: function (data) {
                            $('#PartialContent').hide().html(data).fadeIn("slow");
                        },
                        error: function (request, status, error) {
                            alert("Error-  Request: " + request + ", Status: " + status + ", Error: " + error);
                        },
                        complete: function () {
                            //$('div.sidenav').unblock();
                        }
                    });
                }

              

            });
    </script>

</body>
</html>
